<%= render "users/list", locals: {user: @user, book: @book} %>
<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= link_to user_path(user.id) do %>
          <%= attachment_image_tag user.profile_image, :profile_image, :fill, 60, 60, format: 'jpeg', fallback: "no_image.jpg" %>
        <% end %>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= link_to "Show", user_path(user.id) %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>

<p>copyright</p>

<% %>